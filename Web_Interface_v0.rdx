												<EXP><![CDATA[$C$SELECT CONCAT(M.ORIGIN_DB, ' - ',  M.ORIGIN_TABLE) AS ORIGIN, 
	CASE
		WHEN M.ORIGIN_ID_1<>'' THEN
			STUFF(( 
			SELECT '/' + CONCAT(M1.ORIGIN_ID_0, ' & ', M1.ORIGIN_ID_1) 
			FROM METATRADS as M1 with (NOLOCK)
			WHERE T.IDX=M1.IDX
			FOR XML PATH(''), TYPE).value('(./text())[1]', 'NVARCHAR(MAX)'), 1, 1, '') 
		ELSE
		STUFF((
			SELECT '/' + M1.ORIGIN_ID_0
			FROM METATRADS as M1 with (NOLOCK)
			WHERE T.IDX=M1.IDX
			FOR XML PATH(''), TYPE).value('(./text())[1]', 'NVARCHAR(MAX)'), 1, 1, '') 
		END
	AS TAGS, T.IDX, T.ITA, T.ENG, T.ESP, T.FRA, T.DEU,T.UPDATED, T.NEW AS IS_NEW, MAX(M.LAST_UPDATE) as LAST_UPDATE
FROM TRADS as T with (NOLOCK)
LEFT JOIN METATRADS AS M with (NOLOCK) on T.IDX=M.IDX 
WHERE DELETED IS NULL OR DELETED=0
GROUP BY T.IDX, T.ITA, T.ENG, T.ESP, T.FRA, T.DEU,T.UPDATED, T.NEW, M.ORIGIN_DB,  M.ORIGIN_TABLE, M.ORIGIN_ID_1
ORDER BY T.ITA
]]></EXP>
